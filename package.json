{
  "name": "@bochilteam/scraper",
  "version": "0.0.6",
  "description": "scraper module",
  "exports": {
    ".": {
      "types": "./lib/@types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/@types/index.d.ts",
  "scripts": {
    "test:cjs": "npm run build:cjs && mocha --timeout 50000 ./lib/cjs/**/test.js",
    "test:esm": "npm run build:esm && mocha --timeout 50000 ./lib/esm/**/test.js",
    "test": "npm run build && mocha --timeout 50000 ./lib/**/**/test.js",
    "build:cjs": "echo 'build:cjs' && tsc --project tsconfig.cjs.json && echo 'build:cjs-success'",
    "build:esm": "echo 'build:esm' && ttsc --project tsconfig.esm.json && echo 'build:esm-success'",
    "build:types": "echo 'build:types' && tsc --project tsconfig.types.json && echo 'build:types-success'",
    "build": "echo 'build' && npm install --ignore-scripts -D && npm run build:cjs && npm run build:esm && npm run build:types && echo 'build-success'",
    "prepare": "echo 'prepare' && npm run build && echo 'prepare-success'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BochilTeam/scraper.git"
  },
  "author": "BochilTeam",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/BochilTeam/scraper/issues"
  },
  "homepage": "https://github.com/BochilTeam/scraper#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.9",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "chai": "^4.3.6",
    "mocha": "^9.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "form-data": "^4.0.0",
    "got": "^11.8.3",
    "human-readable": "^0.2.1"
  },
  "directories": {
    "example": "example",
    "src": "src",
    "test": "test"
  },
  "optionalDependencies": {
    "ws": "^8.0.0"
  }
}
